<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="closepane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="100.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.Graphic.Controller.ConfirmGodController">
   <children>
      <Label alignment="CENTER" layoutX="40.0" layoutY="14.0" prefHeight="20.0" prefWidth="220.0" text="Are you sure to play with these Gods?" />
      <Button fx:id="confirm_btn" layoutX="70.0" layoutY="50.0" mnemonicParsing="false" onMouseClicked="#confirm" prefHeight="30.0" prefWidth="60.0" text="Confirm">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Button fx:id="cancel_btn" layoutX="170.0" layoutY="50.0" mnemonicParsing="false" onMouseClicked="#cancel" prefHeight="30.0" prefWidth="60.0" text="Cancel">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </children>
</AnchorPane>
